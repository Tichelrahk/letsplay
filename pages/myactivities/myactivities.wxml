<!--pages/myactivities/myactivities.wxml-->


<!--pages/eventshow/eventshow.wxml-->
<view class="container">

<view class="swiper-tab">  
    <view class="swiper-tab-list {{currentTab==0 ? 'on' : ''}}" data-current="0" bindtap="swichNav">Attending</view>  
    <view class="swiper-tab-list {{currentTab==2 ? 'on' : ''}}" data-current="2" bindtap="swichNav">Organizing</view>  
</view>  
  
<swiper current="{{currentTab}}" class="swiper-box" duration="300" style="height:{{winHeight - 31}}px" bindchange="bindChange">  
    <swiper-item>  
      <view></view>  
    </swiper-item>  
    
    <swiper-item>  
      <view>HOST</view>  
    </swiper-item>  
</swiper>  
</view>
