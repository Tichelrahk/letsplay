/* pages/useredit/useredit.wxss */

.label-list {
    display: flex;
    width: 100%;
    height: 80rpx;
    align-items: center;
    border-bottom: 1px solid #e8e8e8;
}
.place {
    display: flex;
    align-items: center;
    font-size: 36rpx;
} 
.label-list .label {
    color: #333;
    font-size: 36rpx;
}
.label-list input{
    padding-left: 20rpx;
    flex: 1;
    color: #333;
    font-size: 36rpx;
}
.label-list image {
    height: 80rpx;
    width: 80rpx;
}
.label-lists {
    justify-content: space-between;
}

.radio-group label{
    padding-left: 30rpx;
    font-size: 36rpx;
}

.btn2{
  margin:50%;
}

.flex-grow {
  flex-grow: 1;
}

.w-100 {
  width: 100%;
}